  //WRITE ME
  // if (line != NULL)
  country_t ans;
  // ans.name[0] = '\0';
  // ans.population = 0;
  char * ptr = line;
  int comma = 0;
  while (*ptr != ','){
    ptr++;
    comma++;
  }
  for (int i=0; i<comma; i++){
    ans.name[i] = line[i];
  }
  for (int j=comma;j < strlen(line); j++){
    
  }
  
  return ans;
}

